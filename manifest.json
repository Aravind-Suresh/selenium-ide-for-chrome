{
	"name": "SeleniumIDE for Chrome",
	"description": "SeleniumIDEのChrome移植版",
	"manifest_version": 2,
	"version": "0.1",
	"browser_action": {
		"default_title" : "start",
		"default_icon": "/src/popup/icon128.png",
		"default_popup": "/src/popup/index.html"
	},
	"permissions": [
		"http://localhost:9515/",
		"tabs"
	],
	"icons": {
		"48": "/src/icon48.png"
	}
}